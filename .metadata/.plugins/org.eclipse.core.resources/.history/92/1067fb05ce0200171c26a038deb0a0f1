import org.junit.Assert.assertEquals;

import org.junit.Test;
public class Customer_Tester
{
   private Customer testRec = new Customer("Mr", "connor1", "curran0", "0877718912", "stephencurrangmail.com");
   
   @Test
   public void read()
   {
     testRec.EmailAddress("stephencurranGmail.com");
 	 assertEquals("stephencurranGmail.com", testRec.EmailAddress());
   }
   
   
}
